{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerServices_acs_test_name": {
            "defaultValue": "acs-test",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/a4e320e6-d0fe-47dc-84ba-2049e1f6d4b5/resourcegroups/test/providers/Microsoft.ContainerService/containerServices/acs-test'.",
            "type": "Microsoft.ContainerService/containerServices",
            "name": "[parameters('containerServices_acs_test_name')]",
            "apiVersion": "2016-03-30",
            "location": "southindia",
            "scale": null,
            "properties": {
                "orchestratorProfile": {
                    "orchestratorType": "Swarm"
                },
                "masterProfile": {
                    "count": 1,
                    "dnsPrefix": "[concat(parameters('containerServices_acs_test_name'),'mgmt')]"
                },
                "agentPoolProfiles": [
                    {
                        "name": "agentpool",
                        "count": 1,
                        "vmSize": "Standard_F2s",
                        "dnsPrefix": "[concat(parameters('containerServices_acs_test_name'),'agents')]"
                    }
                ],
                "linuxProfile": {
                    "adminUsername": "acs-admin",
                    "ssh": {
                        "publicKeys": [
                            {
                                "keyData": "ssh-rsa "
                            }
                        ]
                    }
                }
            },
            "dependsOn": []
        }
    ]
}
